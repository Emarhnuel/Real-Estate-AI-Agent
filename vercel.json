{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "CLERK_JWKS_URL": "@clerk_jwks_url",
    "TAVILY_API_KEY": "@tavily_api_key",
    "MAPBOX_ACCESS_TOKEN": "@mapbox_access_token",
    "POSTGRES_URL": "@postgres_url"
  }
}
